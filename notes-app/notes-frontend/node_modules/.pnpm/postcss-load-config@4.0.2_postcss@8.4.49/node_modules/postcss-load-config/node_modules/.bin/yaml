#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/yaml@2.6.1/node_modules/yaml/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/yaml@2.6.1/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules:/home/theia/notes-app/notes-app/node_modules:/home/theia/notes-app/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/postcss-load-config@4.0.2_postcss@8.4.49/node_modules/postcss-load-config/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/postcss-load-config@4.0.2_postcss@8.4.49/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/yaml@2.6.1/node_modules/yaml/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/yaml@2.6.1/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules:/home/theia/notes-app/notes-app/node_modules:/home/theia/notes-app/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/postcss-load-config@4.0.2_postcss@8.4.49/node_modules/postcss-load-config/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/postcss-load-config@4.0.2_postcss@8.4.49/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../yaml@2.6.1/node_modules/yaml/bin.mjs" "$@"
else
  exec node  "$basedir/../../../../../yaml@2.6.1/node_modules/yaml/bin.mjs" "$@"
fi
