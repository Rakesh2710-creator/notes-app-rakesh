#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/eslint@8.57.1/node_modules/eslint/bin/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/eslint@8.57.1/node_modules/eslint/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/eslint@8.57.1/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules:/home/theia/notes-app/notes-app/node_modules:/home/theia/notes-app/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/@babel+eslint-parser@7.25.9_@babel+core@7.26.0+eslint@8.57.1/node_modules/@babel/eslint-parser/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/@babel+eslint-parser@7.25.9_@babel+core@7.26.0+eslint@8.57.1/node_modules/@babel/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/@babel+eslint-parser@7.25.9_@babel+core@7.26.0+eslint@8.57.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/eslint@8.57.1/node_modules/eslint/bin/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/eslint@8.57.1/node_modules/eslint/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/eslint@8.57.1/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules:/home/theia/notes-app/notes-app/node_modules:/home/theia/notes-app/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/@babel+eslint-parser@7.25.9_@babel+core@7.26.0+eslint@8.57.1/node_modules/@babel/eslint-parser/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/@babel+eslint-parser@7.25.9_@babel+core@7.26.0+eslint@8.57.1/node_modules/@babel/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/@babel+eslint-parser@7.25.9_@babel+core@7.26.0+eslint@8.57.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../eslint@8.57.1/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../../eslint@8.57.1/node_modules/eslint/bin/eslint.js" "$@"
fi
