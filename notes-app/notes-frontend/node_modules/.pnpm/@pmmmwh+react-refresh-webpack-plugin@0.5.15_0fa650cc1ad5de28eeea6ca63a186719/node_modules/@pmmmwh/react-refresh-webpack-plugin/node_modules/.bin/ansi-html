#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/ansi-html@0.0.9/node_modules/ansi-html/bin/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/ansi-html@0.0.9/node_modules/ansi-html/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/ansi-html@0.0.9/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules:/home/theia/notes-app/notes-app/node_modules:/home/theia/notes-app/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.5.15_0fa650cc1ad5de28eeea6ca63a186719/node_modules/@pmmmwh/react-refresh-webpack-plugin/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.5.15_0fa650cc1ad5de28eeea6ca63a186719/node_modules/@pmmmwh/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.5.15_0fa650cc1ad5de28eeea6ca63a186719/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/ansi-html@0.0.9/node_modules/ansi-html/bin/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/ansi-html@0.0.9/node_modules/ansi-html/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/ansi-html@0.0.9/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules:/home/theia/notes-app/notes-app/node_modules:/home/theia/notes-app/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.5.15_0fa650cc1ad5de28eeea6ca63a186719/node_modules/@pmmmwh/react-refresh-webpack-plugin/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.5.15_0fa650cc1ad5de28eeea6ca63a186719/node_modules/@pmmmwh/node_modules:/home/theia/notes-app/notes-app/notes-frontend/node_modules/.pnpm/@pmmmwh+react-refresh-webpack-plugin@0.5.15_0fa650cc1ad5de28eeea6ca63a186719/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../ansi-html@0.0.9/node_modules/ansi-html/bin/ansi-html" "$@"
else
  exec node  "$basedir/../../../../../../ansi-html@0.0.9/node_modules/ansi-html/bin/ansi-html" "$@"
fi
